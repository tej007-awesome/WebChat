OPENROUTER_API_KEY=your-openrouter-key

# Qdrant (cloud or local). If you're using local Qdrant, leave QDRANT_API_KEY empty.
QDRANT_URL=https://your-cluster.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key

# Frontend -> backend URL (only needed for Docker / non-default setups)
API_BASE=http://localhost:8000

# Logging
LOG_DIR=logs
LOG_LEVEL=INFO
LOG_TO_CONSOLE=1
